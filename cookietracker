<?php
$value = 'something from somewhere';

setcookie("TestCookie", $value);
setcookie("TestCookie", $value, time()+3600*24*30);  /* expire in 30 days
setcookie("TestCookie", $value, time()+3600*24*30, "/~rasmus/", "Krish-Gupta4.github.io", 1);
header('Location: https://krish-gupta4.github.io/');
exit;
?>

<?php
// Print an individual cookie
echo $_COOKIE["TestCookie"];

// Another way to debug/test is to view all cookies
print_r($_COOKIE);
?>



